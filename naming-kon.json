{
  "basePackage": "github.com/spilikin/koap-go/api",
  "packageMappings": [
    {
      "pattern": "de\\.gematik\\.ws\\.(.*)",
      "replacement": "gematik.$1"
    },
    {
      "pattern": "oasis.names.tc.([^.]+).([^.]+)",
      "replacement": "oasis.$1$2"
    },
    {
      "pattern": "org\\.(.*)",
      "replacement": "$1"
    }
  ]
}
